unit SystemAuthentificationService;

interface

uses

  ApplicationService,
  ClientLogOnInfo,
  ServiceInfo,
  ClientAccount,
  SysUtils,
  Classes;

type

  ISystemAuthentificationService = interface

    function Authentificate(
      ClientLogOnInfo: TClientLogOnInfo;
      ServiceInfo: TServiceInfo
    ): TClientAccount;

  end;
  
implementation

end.
