  TForm3 = class(TForm)
    Button1: TButton;
    procedure Button1Click(Sender: TObject);
  private

    procedure TestOleExcelTableData;

  public

  end;

var
  Form3: TForm3;

implementation

uses

  AuxDebugFunctionsUnit;
  
{$R *.dfm}

procedure TForm3.Button1Click(Sender: TObject);
begin

  TestOleExcelTableData;

end;

procedure TForm3.TestOleExcelTableData;
var Reader: TOLEExcelTableData;
    I, J: Integer;
    Value: Variant;
begin

  Reader :=
    TOLEExcelTableData.Create(
      'C:\Users\admin\Documents\Code Gear\Projects\' +
      'FuelCharacteristicsAccountingSystem\Справочные' +
      ' данные\Резервуары\ГТ_ЦистернаТип62.xlsx'
    );

  try

    for I := 0 to Reader.RowCount - 1 do begin

      for J := 0 to Reader.ColumnCount - 1 do begin

        Value := Reader[I, J];
        
      end;

    end;

    Reader[1, 1] := 'TEST SM';
    
  finally

    FreeAndNil(Reader);

  end;

end;

end.
